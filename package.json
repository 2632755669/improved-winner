{
  "name": "awp-react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "PUBLIC_URL=build/pc/ PUBLIC_PATH=build node ./bin/talos_build",
    "build:code": "craco build",
    "commit": "git cz",
    "cov": "echo missing coverage",
    "dev": "npm start",
    "eject": "react-scripts eject",
    "newbranch": "npx git newbranch",
    "start": "craco start",
    "lint": "yarn run lint:script && yarn run lint:style && yarn run prettier",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx,.vue .",
    "lint:style": "stylelint **/*.{less,vue}",
    "lint-fix": "yarn run lint-fix:script && yarn run lint-fix:style && yarn run prettier:fix",
    "lint-fix:script": "npm run lint:script -- --fix",
    "lint-fix:style": "npm run lint:style -- --fix",
    "prettier": "prettier -c \"./src/**/*\"",
    "prettier:fix": "prettier --write -l \"./src/**/*\"",
    "tailwind-watch": "tailwindcss -i ./src/styles/index.css -o ./dist/index.css --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cap/mtv-fetch": "0.1.1",
    "@craco/craco": "6.4.0",
    "@mtfe/sso-client": "2.4.1",
    "@ss/mtd-react": "1.1.15",
    "axios": "0.24.0",
    "classnames": "2.3.1",
    "craco-less": "1.20.0",
    "js-cookie": "3.0.1",
    "lodash-es": "4.17.21",
    "lodash-uuid": "0.0.3",
    "node-polyfill-webpack-plugin": "2.0.1",
    "postcss-px2rem": "0.3.0",
    "react": "^17",
    "react-dom": "^17",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "rxjs": "7.4.0",
    "styled-components": "5.3.3",
    "swiper": "8.4.5",
    "tailwindcss": "3.2.4",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "@block/eslint-config": "0.5.10",
    "@mfe/precommit-eslint": "2.1.3",
    "@types/lodash-es": "4.17.5",
    "@types/node": "12.0.2",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/react-router-dom": "5.3.2",
    "@types/styled-components": "5.1.15",
    "env-cmd": "9.0.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.8.1",
    "px2rem-loader": "0.1.9",
    "stylelint": "^13"
  },
  "resolutions": {
    "@types/react": "17.0.44"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx,json,css,less,scss,html,md}": [
      "prettier --write -l",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "src/**/*.(less|scss)": "stylelint"
  }
}
